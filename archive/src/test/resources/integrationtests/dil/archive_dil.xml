<dil>
	<integrations>
		<integration>
			<flows>
				<flow>
					<id>archive_test1</id>
					<name>archive_test1</name>
					<type>esb</type>
					<steps>
						<step>
							<id>1</id>
							<type>source</type>
							<uri>https://0.0.0.0:9001/1/Archive</uri>
						</step>
						<step>
							<id>2</id>
							<type>action</type>
							<uri>setbody:message:archive</uri>
						</step>
						<step>
							<id>3</id>
							<type>action</type>
							<uri>zip</uri>
							<options>
								<usingIterator>true</usingIterator>
								<preservePathElements>true</preservePathElements>
							</options>
						</step>
						<step>
							<id>4</id>
							<type>action</type>
							<uri>log</uri>
						</step>
						<step>
							<id>5</id>
							<type>action</type>
							<uri>unzip</uri>
							<options>
								<usingIterator>true</usingIterator>
							</options>
						</step>
						<step>
							<id>6</id>
							<type>action</type>
							<uri>log</uri>
						</step>
						<step>
							<id>7</id>
							<type>sink</type>
							<uri>velocity:Message Body: ${bodyAs(String)}</uri>
						</step>
					</steps>
				</flow>
			</flows>
		</integration>
	</integrations>
	<core>
		<messages>
			<message>
				<id>archive</id>
				<body>Hello</body>
			</message>
		</messages>
	</core>
</dil>